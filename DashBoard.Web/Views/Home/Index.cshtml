@{
    ViewBag.Title = "Home Page";
}
@model UniversitiesDashBoardDemo.Models.DashBoardModel
@section scripts
{
    <script>
        $(document).ready(function () {
            $("#chartType").select2();
            $("#drpsearch").select2({
                dropdownParent: $('#section-settings')
            }
            );
        });
    </script>
}
@*<div class="loader"></div>*@
<section class="wrapper main-wrapper row" style=''>
    @Html.Partial("_DashboardAll", Model)
</section>
<script>
    $(document).ready(function () {
        $("#drpsearch").change(function () {
            $("#SearchCriteria").val($("#drpsearch").val());
            $("a#search").each(function (i) { $(this).trigger('click'); });

        });
    });
</script>


